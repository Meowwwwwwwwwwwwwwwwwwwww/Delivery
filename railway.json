{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "name": "Midnight-Munch-API",
  "services": [
    {
      "name": "web",
      "project": "django-service",
      
      
      "buildCommand": "pip install -r requirements.txt && bash ./build.sh",
      
     
      "startCommand": "python delivery/manage.py migrate --no-input && gunicorn delivery.wsgi:application --bind 0.0.0.0:$PORT"
    }
  ],
  "dependencies": {}
}
